{
   "background": {
      "persistent": false,
      "scripts": [ "valuetime.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "home.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Replace new tab page with ValueTime",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": "ValueTime",
   "offline_enabled": true,
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
